.content {
	min-height: 400px;
}

.filebrowser {
	width: 100%;
	float: left;
	min-height: 300px;
}

.filemanager_row {
	border: 1px solid #cecece;
	width: 100%;		
	float: left;
	height: 31px;
	padding: 2px;
	cursor: pointer;
}

.filemanager_row:hover {
	background-color: #E9F0F2;
}

.filebrowser .filemanager_row:first-child {
	border-bottom: none;
}

.filebrowser .filemanager_row:last-child {
	border-bottom: 1px solid #cecece;
}

.filemanager_icon {	
	float: left;	
	height: 100%;
}

.filemanager_icon img {
	width: 32px;
	height: 32px;
	margin-left: 10px;
	margin-right: 10px;
	float: left;	
}


.filemanager_label {	
	margin-top: 8px;
	margin-left: 30px;
	float: left;
}

