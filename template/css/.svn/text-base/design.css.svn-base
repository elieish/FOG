}

.footer .col1 img {
	margin-bottom: 9px;
}

.footer .col2 {
	width: 246px;
	height: 100%;
	float: left;
}

.footer .col3 {
	width: 246px;
	height: 100%;
	float: left;
}

.footer .col4 {
	width: 246px;
	height: 100%;
	float: left;
}

/* ----------------------
 * END: Footer
