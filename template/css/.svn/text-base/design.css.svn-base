/* ----------------------
 * Home Page
 * ---------------------- */

.top {
	width: 100%;
	height: 326px;
	background: url(../img/slider_bg.jpg) repeat-x;
}

/* Slideshow */
#slides {
	position: relative;
}

.pagination {
	display: none;
}

.slides_container {
	width: 920px;
	height: 270px;
	display:none;
	overflow: hidden;
	margin: 0 auto;
	top: 20px;
	
	-moz-box-shadow: 0 2px 6px #000;
	-webkit-box-shadow: 0 2px 6px #000;
	box-shadow: 0 2px 6px #000;
}

.slide {
	width: 920px;
	height: 270px;
	color: #000;
	background: #f88f30;
	color: #fff;
	font-weight: bold;
}

.slide img {
	border: none;
	margin: 0;
	padding: 0;
}

.slide .left {
	width: 600px;
	float: left;
}

.slide .right {
	width: 280px;
	float: right;
	padding: 20px;
	color: #111;
}

#slides .slide .right h2 {
	font-size: 32px;
	line-height: normal;
	border-bottom: none;
	color: #fff;
	text-shadow: 0 1px 0 #000;
}

#slides .prev,
#slides .next {
	display: none;
}

#slides:hover .prev,
#slides:hover .next {
	display: block;
}


.prev {
	position: absolute;
	top: 110px;
	left: 52px;
	padding: 10px 30px;
	background: #ed5727;
	font-size: 14px;
	color: #fff;
	opacity: 0.7;
	z-index: 9999;
}

.next {
	position: absolute;
	top: 110px;
	right: 52px;
	padding: 10px 30px;
	background: #ed5727;
	font-size: 14px;
	color: #fff;
	opacity: 0.7;
	z-index: 9999;
}

.prev:hover,
.next:hover {
	opacity: 1;
	text-shadow: 0 1px 0 #002615;
}

#slides a:hover {
	text-decoration: none;
}

.bottom img {
	margin-left: 0;
}
 
.bottom .col1 {
	width: 300px;
	float: left;
	padding: 18px 20px;
}

.bottom .col2 {
	width: 300px;
	float: left;
	padding: 18px 20px;
}

.bottom .col3 {
	width: 300px;
	float: left;
	padding: 18px 20px;
}


/* ----------------------
 * END: Home Page
 * ---------------------- */
 
 /* ----------------------
 * Export Client Profile
 * ---------------------- */

.row:before,
.row:after {
	clear: both;
	display: table;
	content: "";
	visibility: hidden;
}

.breadcrumbs ul {
	margin: 0;
}

.breadcrumbs li {
	display: inline;
	color: #ED5627;
}

.breadcrumbs li a {
	color: #909090;
}

.row .col h2 {
	margin-top: 10px;
}

.row .col {
	padding: 18px 34px;
    width: 260px;
    float: left;
}

.row .col:first-child {
	padding: 18px 27px 18px 0px;
}

.row .col a {
	margin-bottom: 10px;
}

.row .col a:hover {
	text-decoration: none;
}

.row .col a img {
	opacity: 0.7;
}

.row .col a:hover img {
	opacity: 1;
}

.row .col img,
.row .col img a {
	border: 1px solid #C5C5C5;
	margin: 0px;
}

.row-pag ul {
	max-width: 100%;
}

.row-pag li,
.row-pag li a {
	color: #ED5727;
	display: inline;
	font-size: 16px;
	padding: 0 1px;
}

.row-pag .button {
	color: #FFFFFF;
	margin: 0 14px;
	font-size: 16px;
}

/* Table */

.table {
	margin-top: 20px;
	width: 982px;
}

.table img {
	margin: 0px;
}

.table p {
	width: 278px;
}

/* ----------------------
 * END: Export Client Profile
 * ---------------------- */
 
