/* ----------------------
 * Directors Dinner
 * ---------------------- */
.director_photos {
	width: 600px;
}

.director_photos h1 {
	margin-top: 40px;
}

.director_photos img {
	margin: 0;
	padding: 0;
}

.director_photos_description {
	float: left;
	font-style: italic;
	font-weight: bold;
	color: #555;
	margin-bottom: 30px;
}



div.pagination {
	padding: 3px;
	margin: 3px;
}

div.pagination a {
	padding: 2px 5px 2px 5px;
	margin: 2px;
	border: 1px solid #AAAADD;
	
	text-decoration: none; /* no underline */
	color: #000099;
}
div.pagination a:hover, div.pagination a:active {
	border: 1px solid #000099;

	color: #000;
}
div.pagination span.current {
	padding: 2px 5px 2px 5px;
	margin: 2px;
		border: 1px solid #000099;
		
		font-weight: bold;
		background-color: #000099;
		color: #FFF;
	}
	div.pagination span.disabled {
		padding: 2px 5px 2px 5px;
		margin: 2px;
		border: 1px solid #EEE;
	
		color: #DDD;
	}



/* ----------------------
 * END: Directors Dinner
 * ---------------------- */

