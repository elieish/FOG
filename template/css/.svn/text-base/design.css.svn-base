/* ----------------------
 * Wrapper
 * ---------------------- */

.wrapper {
	width: 1024px;
	height: auto;
	margin: 0 auto 32px;
	border: 1px solid #666;
	border-top: none;
}

/* ----------------------
 * END: Wrapper
 * ---------------------- */

/* ----------------------
 * Header
 * ---------------------- */

.header {
	height: 100px;
	background: #fff;
}

.header h3 {
	font-weight: bold;
}

.logo {
	padding: 10px 20px;
	float: left;
}

.quick_contact {
	float: left;
	color: #4c4c4c;
	font-size: 12px;
	padding: 20px;
}

.quick_contact a {
	color: #4c4c4c; 
}

.dti {
	float: right;
	padding: 10px 40px;
}

/* ----------------------
 * END: Header
 * ---------------------- */
 
/* ----------------------
 * Navbar
 * ---------------------- */ 
 
.nav {
	float: left;
	clear: both;
	background: url(../img/nav_bg.png) repeat-x;
	border: 1px solid #ed5727;
	border-left: none;
	border-right: none;
	width: 1024px;
}

.nav ul {
	width: 1024px;
}

.nav li {
	display: inline;
}

.nav li a {
	float: left;
	padding: 9px 10px;
	color: #fff;
	font-size: 14px;
	text-shadow: 0 1px 0 #333;
	border-right: 1px solid #ed5727;
	border-left: 1px solid #f88f30;
}

.nav li:first-child a {
	border-left: none;
}

.nav li a:hover {
	text-decoration: none;
	background: url(../img/nav_bg_hover.png)
}

.nav li a:active {
	padding: 10px 10px 8px;
}
 
/* ----------------------
 * END: Navbar
 * ---------------------- */

/* ----------------------
 * Content
 * ---------------------- */
 
.content {
	clear: both;
	background: #fff;
	padding: 32px 40px 64px;
	background: url(../img/grey_grad.png) repeat-x bottom #fff;
	/*background: url(../img/bl.jpg);*/
}

.content-index  {
	background: url("../img/grey_grad.png") repeat-x scroll center bottom #FFFFFF;
    clear: both;
	padding: 0 0 32px;
}

.content h1 {
	font-size: 32px;
	color: #ed5727;
	font-weight: bold;
	margin-bottom: 25px;
}

.content h2 {
	font-size: 18px;
	color: #ed5727;
	font-weight: bold;
	margin-bottom: 18px;
}

.content h3 {
	color: #333;
	font-weight: bold;
	font-size: 14px;
	margin-top: 18px;
	margin-bottom: 2px;
}

.content h4 {
	font-size: 15px;
	font-weight: bold;
}

.content ol a,
.content li a {
	color: #000;
}

.content ol,
.content ul {
	margin-left: 18px;
	margin-bottom: 18px;
	max-width: 800px;
}

.content img {
	margin: 3px 15px 0;
}

.content strong {
	color: #222;
}

.content iframe {
	border: 1px solid #333;
	margin-bottom: 32px;
}

.content-index  h2 {
	color: #ed5727;
	font-weight: bold;
	font-size: 18px;
	margin-bottom: 10px;
	padding-bottom: 8px;
	border-bottom:1px solid #999;
}

.content-index  h3 {
	font-weight: bold;
	font-size: 14px;
	color: #444;
}

.content p, .content-index p {
	margin-bottom: 18px;
}

.button {
    background: none repeat scroll 0 0 #ED5727;
    border-radius: 3px 3px 3px 3px;
    clear: both;
    color: #FFFFFF;
    display: inline-block;
    margin-bottom: 25px;
    padding: 6px 14px 4px;
    text-align: center;
}

.button img {
	border: none;
	margin: 0;
	margin-right: 8px;
	padding: 0;
}

.button:hover {
	background: #f88f30;
	text-decoration: none;
}

.red p, .red a {
	color: #D80000;
	max-width: 900px;
	font-weight: bold;
}

/* ----------------------
 * END: Content
 * ---------------------- */ 

